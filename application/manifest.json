{
   "manifest_version": 2,
   "background": {"page":"html/background.html"},
   "browser_action": {
      "default_icon": "img/icon.png"
   },
   "description": "File controller",
   "icons": {
      "128": "img/icon.png"
   },
   "name": "File Controller",
   "options_page": "html/options.html",
   "content_security_policy": "script-src 'self' https://nodejs.boszdigital.com:8877 https://localhost:8877; object-src 'self'",
   "permissions": [
      "tabs", 
      "webRequest", 
      "*://*.googleapis.com/", 
      "*://*.nodejs.boszdigital.com/",
      "*://*.localhost/", 
      "notifications", 
      "background" 
   ],
   "web_accessible_resources": [
      "img/icon.png"
    ],
   "version": "0.0.0.7"
}
